{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ghostty",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "directive"
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": "#"
            }
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^\\r\\n]*"
            },
            "named": true,
            "value": "text"
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          }
        ]
      }
    },
    "directive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "basic_directive"
        },
        {
          "type": "SYMBOL",
          "name": "palette_directive"
        },
        {
          "type": "SYMBOL",
          "name": "config_file_directive"
        },
        {
          "type": "SYMBOL",
          "name": "keybind_directive"
        }
      ]
    },
    "basic_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "property",
          "content": {
            "type": "SYMBOL",
            "name": "property"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "value"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "_kebab_case_identifier": {
      "type": "PATTERN",
      "value": "[a-z]+(-[a-z]+)*"
    },
    "_snake_case_identifier": {
      "type": "PATTERN",
      "value": "[a-z\\_]+\\d*"
    },
    "property": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_kebab_case_identifier"
        }
      ]
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "boolean_literal"
        },
        {
          "type": "SYMBOL",
          "name": "number_literal"
        },
        {
          "type": "SYMBOL",
          "name": "adjustment"
        },
        {
          "type": "SYMBOL",
          "name": "color_value"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "raw_value"
        }
      ]
    },
    "boolean_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "number_literal": {
      "type": "PATTERN",
      "value": "[0-9]+(\\.[0-9]+)?"
    },
    "adjustment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "percent_adjustment"
        },
        {
          "type": "SYMBOL",
          "name": "numeric_adjustment"
        }
      ]
    },
    "string_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "PATTERN",
              "value": "[^\"]*"
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "PATTERN",
              "value": "[^']*"
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        }
      ]
    },
    "color_value": {
      "type": "PATTERN",
      "value": "#?[0-9a-fA-F]{6}"
    },
    "percent_adjustment": {
      "type": "PATTERN",
      "value": "[+-]?[0-9]+(\\.[0-9]+)?%"
    },
    "numeric_adjustment": {
      "type": "PATTERN",
      "value": "[+-][0-9]+(\\.[0-9]+)?"
    },
    "raw_value": {
      "type": "PATTERN",
      "value": "[^\\r\\n]+"
    },
    "palette_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "property",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "STRING",
              "value": "palette"
            },
            "named": true,
            "value": "property"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "palette_value"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "palette_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[0-9]{1,3}"
          },
          "named": true,
          "value": "palette_index"
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": "="
          }
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "PATTERN",
              "value": "#?[0-9a-fA-F]{6}"
            }
          },
          "named": true,
          "value": "color_value"
        }
      ]
    },
    "config_file_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "property",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "STRING",
              "value": "config-file"
            },
            "named": true,
            "value": "property"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "path_value"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "path_value": {
      "type": "PATTERN",
      "value": "[^\\r\\n]+"
    },
    "keybind_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "property",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "STRING",
              "value": "keybind"
            },
            "named": true,
            "value": "property"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_keybind_value"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "_keybind_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keybind_value"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "clear"
          },
          "named": true,
          "value": "value"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "string_literal"
          },
          "named": true,
          "value": "value"
        }
      ]
    },
    "keybind_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "keybind_modifier"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "trigger",
          "content": {
            "type": "SYMBOL",
            "name": "keybind_trigger"
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": "="
          }
        },
        {
          "type": "FIELD",
          "name": "action",
          "content": {
            "type": "SYMBOL",
            "name": "keybind_action"
          }
        }
      ]
    },
    "keybind_modifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "modifier",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "all"
              },
              {
                "type": "STRING",
                "value": "global"
              },
              {
                "type": "STRING",
                "value": "local"
              },
              {
                "type": "STRING",
                "value": "unconsumed"
              }
            ]
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": ":"
          }
        }
      ]
    },
    "key_prefix": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "prefix_name",
          "content": {
            "type": "STRING",
            "value": "physical"
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": ":"
          }
        }
      ]
    },
    "keybind_trigger": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "chord"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ">"
              },
              {
                "type": "SYMBOL",
                "name": "chord"
              }
            ]
          }
        }
      ]
    },
    "chord": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "key_modifier"
            },
            {
              "type": "SYMBOL",
              "name": "key"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "+"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "key_modifier"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "key"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "key_modifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "shift"
        },
        {
          "type": "STRING",
          "value": "ctrl"
        },
        {
          "type": "STRING",
          "value": "control"
        },
        {
          "type": "STRING",
          "value": "alt"
        },
        {
          "type": "STRING",
          "value": "option"
        },
        {
          "type": "STRING",
          "value": "opt"
        },
        {
          "type": "STRING",
          "value": "super"
        },
        {
          "type": "STRING",
          "value": "cmd"
        },
        {
          "type": "STRING",
          "value": "command"
        }
      ]
    },
    "key": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "key_prefix"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "bind",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_snake_case_identifier"
              },
              {
                "type": "PATTERN",
                "value": "[^>=:]{1}"
              }
            ]
          }
        }
      ]
    },
    "keybind_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "action_name",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "_snake_case_identifier"
            },
            "named": true,
            "value": "action_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "IMMEDIATE_TOKEN",
                  "content": {
                    "type": "STRING",
                    "value": ":"
                  }
                },
                {
                  "type": "FIELD",
                  "name": "argument",
                  "content": {
                    "type": "ALIAS",
                    "content": {
                      "type": "SYMBOL",
                      "name": "raw_value"
                    },
                    "named": true,
                    "value": "action_argument"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
