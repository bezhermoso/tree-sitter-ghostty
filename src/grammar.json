{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ghostty",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "directive"
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "STRING",
              "value": "#"
            }
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^\\r\\n]*"
            },
            "named": true,
            "value": "text"
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          }
        ]
      }
    },
    "directive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "basic_directive"
        },
        {
          "type": "SYMBOL",
          "name": "palette_directive"
        },
        {
          "type": "SYMBOL",
          "name": "config_file_directive"
        },
        {
          "type": "SYMBOL",
          "name": "keybind_directive"
        }
      ]
    },
    "basic_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "property",
          "content": {
            "type": "SYMBOL",
            "name": "property"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "value"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "_kebab_case_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[0-9a-z]+"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "STRING",
                  "value": "-"
                }
              },
              {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "PATTERN",
                  "value": "[0-9a-z]+"
                }
              }
            ]
          }
        }
      ]
    },
    "_snake_case_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[0-9a-z]+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "STRING",
                  "value": "_"
                }
              },
              {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "PATTERN",
                  "value": "[0-9a-z]+"
                }
              }
            ]
          }
        }
      ]
    },
    "property": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_kebab_case_identifier"
        }
      ]
    },
    "value": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\s+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "boolean"
            },
            {
              "type": "SYMBOL",
              "name": "number"
            },
            {
              "type": "SYMBOL",
              "name": "adjustment"
            },
            {
              "type": "SYMBOL",
              "name": "color"
            },
            {
              "type": "SYMBOL",
              "name": "string"
            }
          ]
        }
      ]
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "[0-9]+(\\.[0-9]+)?"
    },
    "adjustment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "percent_adjustment"
        },
        {
          "type": "SYMBOL",
          "name": "numeric_adjustment"
        }
      ]
    },
    "string": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "PATTERN",
                "value": "[^\"]*"
              },
              {
                "type": "STRING",
                "value": "\""
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "'"
              },
              {
                "type": "PATTERN",
                "value": "[^']*"
              },
              {
                "type": "STRING",
                "value": "'"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^#\\s]"
              },
              {
                "type": "SYMBOL",
                "name": "_raw_value"
              }
            ]
          }
        ]
      }
    },
    "_loose_string": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "PATTERN",
                "value": "[^\"]*"
              },
              {
                "type": "STRING",
                "value": "\""
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "'"
              },
              {
                "type": "PATTERN",
                "value": "[^']*"
              },
              {
                "type": "STRING",
                "value": "'"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "_raw_value"
          }
        ]
      }
    },
    "color": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "#"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "IMMEDIATE_TOKEN",
            "content": {
              "type": "PATTERN",
              "value": "[0-9a-fA-F]{6}"
            }
          }
        ]
      }
    },
    "percent_adjustment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 2,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "-"
                    },
                    {
                      "type": "STRING",
                      "value": "+"
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "[0-9]+(\\.[0-9]+)?"
              }
            },
            {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "STRING",
                "value": "%"
              }
            }
          ]
        }
      }
    },
    "numeric_adjustment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "-"
                },
                {
                  "type": "STRING",
                  "value": "+"
                }
              ]
            },
            {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "[0-9]+(\\.[0-9]+)?"
              }
            }
          ]
        }
      }
    },
    "_raw_value": {
      "type": "PREC",
      "value": -1,
      "content": {
        "type": "PATTERN",
        "value": "[^\\r\\n]+"
      }
    },
    "palette_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "property",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "STRING",
              "value": "palette"
            },
            "named": true,
            "value": "property"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "palette_value"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "palette_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[0-9]{1,3}"
          },
          "named": true,
          "value": "palette_index"
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": "="
          }
        },
        {
          "type": "SYMBOL",
          "name": "color"
        }
      ]
    },
    "config_file_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "property",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "STRING",
              "value": "config-file"
            },
            "named": true,
            "value": "property"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "path_value"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "path_value": {
      "type": "SYMBOL",
      "name": "string"
    },
    "keybind_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "property",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "STRING",
              "value": "keybind"
            },
            "named": true,
            "value": "property"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "SYMBOL",
                "name": "keybind_value"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "keybind_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "STRING",
          "value": "clear"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "keybind_modifier"
                  }
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "FIELD",
              "name": "trigger",
              "content": {
                "type": "SYMBOL",
                "name": "keybind_trigger"
              }
            },
            {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "STRING",
                "value": "="
              }
            },
            {
              "type": "FIELD",
              "name": "action",
              "content": {
                "type": "SYMBOL",
                "name": "keybind_action"
              }
            }
          ]
        }
      ]
    },
    "keybind_modifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "modifier",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "all"
              },
              {
                "type": "STRING",
                "value": "global"
              },
              {
                "type": "STRING",
                "value": "local"
              },
              {
                "type": "STRING",
                "value": "unconsumed"
              }
            ]
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": ":"
          }
        }
      ]
    },
    "key_qualifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "qualifier",
          "content": {
            "type": "TOKEN",
            "content": {
              "type": "STRING",
              "value": "physical"
            }
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": ":"
          }
        }
      ]
    },
    "keybind_trigger": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "chord"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ">"
              },
              {
                "type": "SYMBOL",
                "name": "chord"
              }
            ]
          }
        }
      ]
    },
    "chord": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "modifier_key"
            },
            {
              "type": "SYMBOL",
              "name": "key"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "+"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "modifier_key"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "key"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "modifier_key": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "shift"
        },
        {
          "type": "STRING",
          "value": "ctrl"
        },
        {
          "type": "STRING",
          "value": "control"
        },
        {
          "type": "STRING",
          "value": "alt"
        },
        {
          "type": "STRING",
          "value": "option"
        },
        {
          "type": "STRING",
          "value": "opt"
        },
        {
          "type": "STRING",
          "value": "super"
        },
        {
          "type": "STRING",
          "value": "cmd"
        },
        {
          "type": "STRING",
          "value": "command"
        }
      ]
    },
    "key": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "qualifier",
              "content": {
                "type": "SYMBOL",
                "name": "key_qualifier"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "FIELD",
            "name": "bind",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[0-9a-z]+"
                    },
                    {
                      "type": "REPEAT",
                      "content": {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "IMMEDIATE_TOKEN",
                            "content": {
                              "type": "STRING",
                              "value": "_"
                            }
                          },
                          {
                            "type": "IMMEDIATE_TOKEN",
                            "content": {
                              "type": "PATTERN",
                              "value": "[0-9a-z]+"
                            }
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "type": "PATTERN",
                  "value": "[^>=:\\s]{1}"
                }
              ]
            }
          }
        }
      ]
    },
    "keybind_action": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "action_name",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "_snake_case_identifier"
            },
            "named": true,
            "value": "action_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "IMMEDIATE_TOKEN",
                  "content": {
                    "type": "STRING",
                    "value": ":"
                  }
                },
                {
                  "type": "FIELD",
                  "name": "argument",
                  "content": {
                    "type": "ALIAS",
                    "content": {
                      "type": "SYMBOL",
                      "name": "_action_arg_value"
                    },
                    "named": true,
                    "value": "action_argument"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_action_arg_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "TOKEN",
            "content": {
              "type": "PREC",
              "value": 2,
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "CHOICE",
                        "members": [
                          {
                            "type": "STRING",
                            "value": "+"
                          },
                          {
                            "type": "STRING",
                            "value": "-"
                          }
                        ]
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "IMMEDIATE_TOKEN",
                    "content": {
                      "type": "PATTERN",
                      "value": "[0-9]+(\\.[0-9]+)?"
                    }
                  }
                ]
              }
            }
          },
          "named": true,
          "value": "number"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_loose_string"
          },
          "named": true,
          "value": "string"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
